{
  "name": "botvik",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write .",
    "postinstall": "prisma generate",
    "prisma:seed": "prisma db seed",
    "content:import": "npx ts-node scripts/import-quizzes.ts",
    "content:import:clear": "npx ts-node scripts/import-quizzes.ts --clear",
    "content:list": "npx ts-node -e \"require('dotenv/config'); const {PrismaClient}=require('@prisma/client'); const {PrismaNeon}=require('@prisma/adapter-neon'); (async()=>{const p=new PrismaClient({adapter:new PrismaNeon({connectionString:process.env.DATABASE_URL})}); const q=await p.quiz.findMany({include:{_count:{select:{questions:true}}}}); console.table(q.map(x=>({id:x.id,title:x.title,questions:x._count.questions,active:x.isActive}))); await p.$disconnect();})()\"",
    "ink:compile": "npx tsx scripts/compile-ink.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:security": "playwright test e2e/security.spec.ts",
    "test:lighthouse": "lhci autorun",
    "test:load": "echo 'Install k6: choco install k6 (Windows) or brew install k6 (Mac)' && k6 run load-tests/stress.js",
    "test:load:spike": "k6 run load-tests/spike.js",
    "test:load:soak": "k6 run load-tests/soak.js",
    "test:all": "npm run test && npm run test:e2e",
    "generate:icons": "npx ts-node scripts/generate-icons.ts"
  },
  "dependencies": {
    "@liveblocks/client": "^3.12.1",
    "@liveblocks/node": "^3.12.1",
    "@liveblocks/react": "^3.12.1",
    "@neondatabase/serverless": "^0.10.4",
    "@prisma/adapter-neon": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@rive-app/react-canvas": "^4.25.1",
    "@sentry/nextjs": "^10.32.1",
    "@supabase/supabase-js": "^2.89.0",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.8",
    "@vercel/analytics": "^1.6.1",
    "@vercel/og": "^0.8.6",
    "@vercel/speed-insights": "^1.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "flowbite": "^4.0.1",
    "flowbite-react": "^0.12.14",
    "framer-motion": "^11.18.2",
    "html2canvas": "^1.4.1",
    "inkjs": "^2.3.2",
    "jose": "^6.1.3",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.562.0",
    "next": "^16.1.1",
    "pg": "^8.16.3",
    "posthog-js": "^1.310.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "web-vitals": "^5.1.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@lhci/cli": "^0.14.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^30.2.0",
    "prettier": "^3.4.2",
    "prisma": "^7.2.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
